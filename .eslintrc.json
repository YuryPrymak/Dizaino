{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "wrap-iife": [2, "inside"],
        "keyword-spacing": [2, {"overrides": {
            "if": {"after": false},
            "for": {"after": false},
            "while": {"after": false}
        }}]
    }
}
